  "main": "index.js",
  "scripts": {
    "dev:cef": "parcel serve  ./src/cef/index.html --out-dir client_packages/cef --target browser",
    "build:cef": "del-cli --force client_packages/cef && npx cross-env NODE_ENV=production parcel build ./src/cef/index.html --out-dir client_packages/cef --target browser --no-source-maps --public-url ./",
    "dev:sound": "parcel serve  ./src/cef/sound.html --out-dir client_packages/cef --target browser --port 1237 --global sound",
    "build:sound": "parcel build ./src/cef/sound.html --out-dir client_packages/cef --target browser --global sound --public-url ./",
    "build:server": "parcel build src/server/index.ts --no-minify --out-dir packages/server --target node",
    "build:client": "npx cross-env NODE_ENV=production parcel build src/client/index.ts --no-minify --out-dir client_packages --target node",
    "dev:server": "parcel watch src/server/index.ts --out-dir packages/server --target node",
    "dev:client": "parcel watch src/client/index.ts --out-dir client_packages --target node",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:cef\"",
    "build:lang:de": "node lang.js de && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build:lang:es": "node lang.js es && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build:lang:en": "node lang.js en && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build:lang:kr": "node lang.js kr && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build:lang:pl": "node lang.js pl && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build:lang:rs": "node lang.js rs && npm run build:server && npm run build:client && npm run build:cef && node lang.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\" \"npm run build:cef\""
  },
